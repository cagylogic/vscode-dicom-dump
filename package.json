{
  "name": "vscode-dicom",
  "displayName": "DICOM Tag Dump",
  "description": "Shows the list of DICOM tags in the editor.",
  "icon": "icon.png",
  "version": "0.1.1",
  "publisher": "smikitky",
  "engines": {
    "vscode": "^1.24.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/smikitky/vscode-dicom-dump"
  },
  "bugs": {
    "url": "https://github.com/smikitky/vscode-dicom-dump/issues"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:dicom",
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "dicom.showTags",
        "title": "Show DICOM tags"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "dicom.showTags",
          "group": "dicom",
          "when": "resourceLangId == dicom"
        }
      ]
    },
    "languages": [
      {
        "id": "dicom",
        "extensions": [
          ".dcm",
          ".dicom"
        ],
        "aliases": [
          "dcm"
        ]
      },
      {
        "id": "dicom-dump",
        "extensions": [
          ".dcm-dump"
        ]
      }
    ],
    "grammars": [
      {
        "language": "dicom-dump",
        "scopeName": "text.dicom-dump",
        "path": "./syntaxes/dicom-dump.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "@types/pify": "^3.0.2",
    "prettier": "^1.13.7",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "dicom-data-dictionary": "^0.3.1",
    "dicom-parser": "^1.8.1",
    "pify": "^3.0.0"
  }
}
