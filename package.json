{
  "name": "vscode-dicom",
  "displayName": "vscode-dicom",
  "description": "Shows DICOM tags",
  "version": "0.0.1",
  "publisher": "smikitky",
  "engines": {
    "vscode": "^1.24.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:dicom"
  ],
  "main": "./out/extension",
  "contributes": {
    "languages": [
      {
        "id": "dicom",
        "extensions": [
          ".dcm",
          ".dicom"
        ],
        "aliases": [
          "dcm"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "prettier": "^1.13.7",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "prettier": {
    "singleQuote": true
  }
}
